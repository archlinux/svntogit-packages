pkgname=agetpkg
pkgver=1
pkgrel=1
pkgdesc='Archlinux Archive Get Package'
arch=('any')
url='https://github.com/seblu/agetpkg'
license=('GPL2')
makedepends=('git')
depends=('python' 'python-xdg')
source=("git+https://github.com/seblu/agetpkg#tag=v$pkgver")
md5sums=('SKIP')

package() {
  cd agetpkg
  install -Dm755 agetpkg "$pkgdir/usr/bin/agetpkg"
}

