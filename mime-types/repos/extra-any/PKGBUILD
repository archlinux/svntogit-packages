pkgname=mime-types
pkgver=1.0
pkgrel=3
pkgdesc="Provides /etc/mime.types"
arch=('any')
url="http://www.archlinux.org/"
license=('GPL2')
backup=('etc/mime.types')
source=(mime.types)
md5sums=('aa90248272ba2e9dca0a243746686739')

build() {
	/bin/true
}

package() {
	cd ${srcdir}
	install -Dm644 mime.types ${pkgdir}/etc/mime.types || return 1
}
