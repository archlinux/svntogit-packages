post_install() {
  echo ':: Flash Player reaches end-of-life on December 31, 2020. After this date,'
  echo '   it will be removed from the Arch repos since it is no longer supported.'
}

post_upgrade() {
  if (( $(vercmp $2 32.0.0.465-1) < 0 )); then
    post_install
  fi
}

