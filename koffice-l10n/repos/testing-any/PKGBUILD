    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

  install -d build/nb
  cd build/nb
  cmake ${srcdir}/koffice-l10n-nb-${pkgver} \
    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

