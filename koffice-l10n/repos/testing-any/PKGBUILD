  cd ../..

  install -d build/tr
  cd build/tr
  cmake ${srcdir}/koffice-l10n-tr-${pkgver} \
    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

  install -d build/uk
  cd build/uk
  cmake ${srcdir}/koffice-l10n-uk-${pkgver} \
    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

  install -d build/zh_CN
  cd build/zh_CN
  cmake ${srcdir}/koffice-l10n-zh_CN-${pkgver} \
