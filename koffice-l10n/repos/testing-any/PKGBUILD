    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

  install -d build/pt
  cd build/pt
  cmake ${srcdir}/koffice-l10n-pt-${pkgver} \
    -DCMAKE_BUILD_TYPE=Release \
    -DCMAKE_INSTALL_PREFIX=/usr
  make 
  cd ../..

  install -d build/pt_BR
  cd build/pt_BR
